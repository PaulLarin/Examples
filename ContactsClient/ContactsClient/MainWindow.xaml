<Window x:Class="ContactsClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ContactsClient"
        xmlns:view="clr-namespace:ContactsClient.View"
        mc:Ignorable="d"
        Title="MainWindow"
		DataContext="{StaticResource MainViewModel}" Width="768">
    <Grid>
        <view:ContactsCatalog DataContext="{Binding ContactsCatalogViewModel}" VerticalAlignment="Stretch" Width="Auto" Height="Auto"/>
    </Grid>
</Window>
